FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    xMin
    {
        type            cyclic;
    }

    xMax
    {
        type            cyclic;
    }

    yMin
    {
        type            cyclic;
    }

    yMax
    {
        type            cyclic;
    }

    zMin
    {
        type            cyclic;
    }

    zMax
    {
        type            cyclic;
    }
}

