FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water air);

water
{
    transportModel  Newtonian;
    nu              nu [ 0 2 -1 0 0 0 0 ] 0.001;
    rho             rho [ 1 -3 0 0 0 0 0 ] 1;
}

air
{
    transportModel  Newtonian;
    nu              nu [ 0 2 -1 0 0 0 0 ] 0.001;
    rho             rho [ 1 -3 0 0 0 0 0 ] 1;

}

sigma           sigma [ 1 0 -2 0 0 0 0 ] 1;

curvatureModel gradAlpha;
fillNeighbours 0;
zonalModel false;
useScaling true;
iMax 2;
jMax 1;
nMax 2;
interfaceTol 1e-4;
extendInterface false;

