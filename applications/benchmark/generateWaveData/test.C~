#include <stdio.h>
#include <iostream>
#include <fstream>
#include <string>

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


///////////////////////////////////////////////////////////////////////////////////////

int main(int argc, char *argv[])
{

  FILE *writeTraining;
  writeTraining = fopen ("test.dat","a");

  fprintf(writeTraining,"%.12e,",10.0);
  fprintf(writeTraining,"%.12e,",10.0);
  fprintf(writeTraining,"%.12e",10.0);
  fprintf(writeTraining,"\n");
  fprintf(writeTraining,"%.12e,",10.0);
  fprintf(writeTraining,"%.12e,",10.0);
  fprintf(writeTraining,"%.12e",10.0);
  fprintf(writeTraining,"\n");

  fclose(writeTraining);
  return 0;
}


